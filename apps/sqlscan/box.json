{
    "alias": "main.phar",
    "chmod": "0755",
    "directories": ["assets"],
    "blacklist": [
        "assets/images.gif"
    ],
    "finder": [
        {
            "name": ["*"],
            "exclude": ["test","tests","example","examples"],
            "in": ["vendor","src"]
        }
    ],
    "git-version": "package_version",
    "main": "main.php",
    "output": "build/main.phar",
    "stub": true,
    "algorithm" :"SHA512",
    "compression" :"BZ2"
}
